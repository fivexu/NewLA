#welcome, page {
  width: 100%;
  height: 100%;
}

#welcome {
  position: fixed;
  left: 0;
  top: 0;
  font-size: 0;
}

#welcome image {
  width: 100%;
  height: 100%;
}

#welcome .to_home {
  position: absolute;
  right: 40rpx;
  top: 40rpx;
  z-index: 10;
  width: 100rpx;
	height: 48rpx;
  line-height: 48rpx;
  text-align: center;
	background-color: #ebebeb;
	border-radius: 8rpx;
  color: #595959;
  font-size: 28rpx;
}
