#collect, page {
  height: 100%;
}

#collect {
  padding: 40rpx 20rpx 0;
  overflow: hidden;
  background-color: #f9f9f9;
}

#collect .wrapper {
  position: relative;
  width: 100%;
  margin-bottom: 40rpx;
}

.productList {
  position: relative;
  z-index: 10;
  background-color: #fff;
  border-radius: 25rpx;
  padding: 20rpx;
  overflow: hidden;
}

.productList .status {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  font-size: 0;
}

.productList .status >image {
  width: 200rpx;
  height: 200rpx;
}

.productList .pics {
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  font-size: 0;
}

.productList image {
  width: 210rpx;
  height: 210rpx;
  border-radius: 6rpx;
}

.productList .title {
  padding-top: 36rpx;
  font-size: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  color: #1a1a1a;
}

.productList .title .time {
  font-size: 24rpx;
  font-weight: normal;
  color: rgb(102, 102, 102);
  display: inline-block;
  height: 34rpx;
  line-height: 50rpx;
  vertical-align: bottom;
  float: right;
}

.productList .address {
  margin: 38rpx 0;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #666;
  position: relative;
}

.productList .address {
  margin: 38rpx 0;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: rgb(102, 102, 102);
  position: relative;
  padding-left: 30rpx;
}

.productList .address .label {
  width: 62rpx;
  height: 28rpx;
  line-height: 28rpx;
  background-color: #e6e6e6;
  border-radius: 5rpx;
  text-align: center;
  font-size: 20rpx;
  color: #666;
  float: left;
  margin: 6rpx 0 0 10rpx;
}

.productList .address .label.ac {
  background-color: #fa6639;
  color: #fff;
}

.productList .address image {
  width: 20rpx;
  height: 27rpx;
  position: absolute;
  bottom: 6rpx;
  left: 0;
}

.productList .address .old {
  color: rgb(153, 153, 153);
  text-decoration: line-through;
}

.productList .address .now {
  margin-left: 20rpx;
  color: rgb(250, 102, 57);
  font-weight: bolder;
}

.productList .address .nowAc {
  font-size: 38rpx;
}

.delete_product {
  position: absolute;
  z-index: 1;
  right: -200rpx;
  top: 0;
  width: 160rpx;
  height: 100%;
  border-radius: 25rpx;
  background-color: #fa6639;
  color: #fff;
}

.delete_product .delete {
  width: fit-content;
  height: fit-content;
  position: absolute;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  margin: auto;
}

.phone_to {
  width: 630rpx;
  height: fit-content;
  padding-top: 70rpx;
  background-color: #fff;
  border-radius: 25rpx;
  position: fixed;
  z-index: 1000;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  margin: auto;
  text-align: center;
  color: #1a1a1a;
}

.phone_to .text {
  padding-bottom: 70rpx;
}

.phone_to .btn {
  width: 100%;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  border-top: 1rpx solid #e6e6e6;
}

.phone_to .btn_item {
  flex: 1;
  font-size: 36rpx;
}

.phone_to .btn_item.ac {
  border-left: 1rpx solid #e6e6e6;
  color: #fa6639;
}

.model {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.5);
}

.refresh {
  font-size: 28rpx;
  color: #b3b3b3;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
